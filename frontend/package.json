{
  "name": "cross-review-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --minify false",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json  --noEmit",
    "format": "prettier --check . --ignore-unknown",
    "format:fix": "prettier --write . --ignore-unknown",
    "lint": "tsc --noEmit && eslint . --ext=.ts,.tsx --quiet && stylelint ./src/**/*.scss --quiet",
    "lint:fix": "eslint . --ext=.ts,.tsx --fix && stylelint ./src/**/*.scss --fix",
    "api-types": "node ./scripts/generateApiFromSwaggerSpec.js",
    "prepare": "cd .. && husky frontend/.husky || true"
  },
  "dependencies": {
    "@gravity-ui/components": "^3.4.2",
    "@gravity-ui/date-components": "^2.1.0",
    "@gravity-ui/date-utils": "^2.1.0",
    "@gravity-ui/icons": "^2.9.1",
    "@gravity-ui/uikit": "^6.9.0",
    "@hookform/resolvers": "^3.3.4",
    "@reduxjs/toolkit": "^2.2.2",
    "@vscode/codicons": "^0.0.35",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "monaco-editor": "^0.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-monaco-editor": "^0.55.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-jsx-expressions": "^1.3.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "postcss": "8.4.35",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.5",
    "sass": "1.72.0",
    "stylelint": "^16.3.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.2.1",
    "stylelint-semantic-groups": "1.2.1",
    "stylelint-use-logical-spec": "5.0.1",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-checker": "^0.6.4",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
